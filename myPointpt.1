public class MyPoint 
{
   private int x;
   private int y;
   
   public int getX()
   {
      return x;
   }
   
   public double distance(int a, int b)
   {
      return Math.sqrt(Math.pow(x - a, 2.0) + Math.pow(y - b, 2.0)); 
   }
   
   public int getY()
   {
      return y;
   }
   
   public void setX(int x)
   {
      this.x = x;
   }
   
     public void setY(int y)
   {
      this.y = y;
   }
}
