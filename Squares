import java.util.Scanner;

public class Squares
{
   
   public static void main(String[] args)
   {
      Scanner scan = new Scanner(System.in);
      System.out.println("Enter a positive integer.");
      int number = scan.nextInt();
      scan.close();
      
      int n = 2;
      while(n * n < number)
      {
         n++;
      }
      System.out.println(n);
   }
}
